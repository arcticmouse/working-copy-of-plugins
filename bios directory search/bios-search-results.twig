<section class="container">
	{% for r in results %}
		<div class="container-fluid">
			<div class="col-xs-2">
				<a href="{{ r.url }}">{{ r.thumb }}</a>
			</div>

			<div class="col-xs-10">
				<section class="post_content">
					<h3 class="h2"><a href="{{ r.url }}" rel="bookmark">{{ r.fname }} {{ r.lname }}</a></h3>
					<strong>{{ r.titles }}</strong><br /><br />
					<strong>Department:</strong> {{ r.department }}<br /><br />
					{{ r.phone }}(ph)<br />
					{{ r.fax }} (fax)<br />
					<a href="mailto:{{ r.email }}">{{ r.email }}</a><br /><br />
					<a href="{{ r.url }}">View Profile</a>
				</section>
			</div>
		</div>
		<br /><br />
	{% endfor %}
</section>