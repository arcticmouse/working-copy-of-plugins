					{% endif %}
				</div>
				<div class="col-sm-8">
					<div class="content">
						<a href="{{ r.url }}"><h4>{{ rpanel.title }}</h4></a>
						<p class="snippet">{{ rpanel.exerpt }}</p>
					</div> <!-- research -->
				</div>

			</div><!-- row news story -->
		
		{% endfor %}

	</div><!-- research panel -->





