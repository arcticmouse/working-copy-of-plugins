					{% endif %}
				</div>
				<div class="col-sm-8">
					<div class="content">
						<a href="{{ e.url }}"><h4>{{ epanel.title }}</h4></a>
						<p class="snippet">{{ epanel.exerpt }}</p>
					</div> <!-- education -->
				</div>

			</div><!-- row news story -->
		
		{% endfor %}

	</div><!-- education panel -->



</div> <!-- end tab content -->

		</div> <!-- row-->


